<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tictactoe_java.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button onAction="#onResetButtonClick" text="Reset" />
   <TextField fx:id="winTxt" alignment="CENTER" prefHeight="25.0" prefWidth="2295.0">
      <font>
         <Font size="14.0" />
      </font>
   </TextField>
   <GridPane fx:id="grid" gridLinesVisible="true" prefHeight="300.0" prefWidth="3000.0">

   </GridPane>
</VBox>
